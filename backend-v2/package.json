    "winston": "^3.11.0",
    "joi": "^17.12.0",
    "graphql": "^16.8.1",
    "apollo-server-express": "^3.13.0",
    "graphql-tools": "^9.0.1",
    "dataloader": "^2.2.2",
    "@elastic/elasticsearch": "^8.12.0",
    "openai": "^4.28.0",
    "@anthropic-ai/sdk": "^0.17.1",
    "@pinecone-database/pinecone": "^2.0.0",
    "bull": "^4.12.0",
    "stripe": "^14.14.0",
    "sharp": "^0.33.2",
    "multer": "^2.0.2",
    "uuid": "^9.0.1",
